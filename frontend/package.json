    "vite": "^4.4.11",
    "volar-service-vetur": "^0.0.14",
    "vue": "^3.2.47",
    "vue-apollo": "^3.1.1",
    "vue-chartjs": "^5.2.0",
    "vue-konva": "^3.0.2",
    "vue-router": "^4.2.5",
    "vue-tsc": "^1.8.18"
